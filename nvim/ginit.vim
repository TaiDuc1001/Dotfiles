GuiPopupmenu 0
GuiScrollBar 1
" Enable line
GuiLinespace 0

" Disable qt tab line 
GuiTabline 0

" Set format of tab name 
set guitablabel=\[%N\]\ %t\ %M 

" Set font
set guifont=FiraCode\ NF:h14
" let g:Guifont="DejaVu Sans Mono for Powerline:h13"

" Set key map to paste 
inoremap <silent> <S-Insert> <C-R>+
" nnoremap <C-/> <leader>cc
